<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
  xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
  xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
    https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.9.xsd
    http://www.liquibase.org/xml/ns/dbchangelog-ext
    https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">

  <changeSet id="unique-parcel-sheet-agreements" author="system">
    <sql>CREATE UNIQUE INDEX IF NOT EXISTS idx_agreements_parcel_sheet ON agreements (parcel_id, sheet_id);</sql>
  </changeSet>

   <changeSet id="add-ingest-id-to-agreements" author="system">
    <sql>ALTER TABLE agreements ADD ingest_id TEXT NOT NULL DEFAULT '';</sql>
  </changeSet>

</databaseChangeLog>
