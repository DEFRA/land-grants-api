<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
  xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
  xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
    https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.9.xsd
    http://www.liquibase.org/xml/ns/dbchangelog-ext
    https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">


  <changeSet id="add-ingest-id-to-compatibility-matrix" author="system">
    <sql>ALTER TABLE compatibility_matrix ADD ingest_id TEXT NOT NULL DEFAULT '';</sql>
  </changeSet>

  <changeSet id="grant-select-for-compatibility-matrix" author="system">
    <sql>GRANT insert, update on compatibility_matrix to land_grants_api;</sql>
  </changeSet>

</databaseChangeLog>
